function ClickCombo()
	ClickTimer=180
	if(Combo==1)then
		Click1()
		Combo=2
	elseif(Combo==2)then
		SwordSummon()
		Combo=1
	elseif(Combo==3)then
		Bombs()
		Combo=1
	end
end
	
function VaporTaunt()
local search = "just look up the word search to find the voicelines"
	if Mode == "Troubadour" then Chat"Feel the music.."
	elseif Mode == "MAYHEM" then Chat"MY RAGE BURNS ON!"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [MAYHEM] MY RAGE BURNS ON! ", "All")
	elseif Mode == "LEGMAN" then Chat"LOL IM BETTER THAN THE OLD ME. IVE GROWN 2 SWORDS SINCE THEN!"
	elseif Mode == "CRUELTY" then Chat"Its Over for you..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [CRUELTY] Its Over for you...  ", "All")
	elseif Mode == "CHRONOS" then Chat"Controlling time'"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [CHRONOS] Time is mine! ", "All")
	elseif Mode == "CRANKTON" then Chat"."
	elseif Mode == "COMPLETELY LOST" then Chat"Shattered across time and space."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Completley Lost] Shattered across time and space. ", "All")
	elseif Mode == "???" then Chat "ERROR"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [???] ___ __ _______  ", "All")
	elseif Mode == "O B L I V I O N" then Chat"."
	elseif Mode == "SHATTERED" then Chat"SHATTER."
	elseif Mode == "BEYOND" then Chat"."
	elseif Mode == "The Doctor" then Chat"Well Well Well. Look what we have here..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [The Doctor] Well Well Well. Look what we have here... ", "All")
	elseif Mode == "Hypnotic" then Chat"I will control you."
		            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Hypnotic] I will control you. ", "All")
	elseif Mode == "ILLUMINATION" then Chat"Prepare for your demise."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [ILLUMINATION] Prepare for your demise  ", "All")
	elseif Mode == "ULTRAVIOLET" then Chat"You will die. No need to run..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [ULTRAVIOLET] You will die. No need to run...  ", "All")
	elseif Mode == "DEATH" then Chat "Death is not an escape..."
		            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [DEATH] Death is not an escape... ", "All")
	elseif Mode == "OMNIVERSAL" then Chat "..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [OMNIVERSAL] THE ENERGY SURGING IS IMMENSE! ", "All")
	elseif Mode == "Galactic" then Chat "TO INFINITY!"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Galactic] TO INFINITY! ", "All")
	elseif Mode == "Galaxy" then Chat "TO THE STARS AND BEYOND!"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Galaxy] TO THE STARS AND BEYOND! ", "All")
	elseif Mode == "SPACETIME" then Chat "I control all of time and space"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [SPACETIME] I control all of time and space  ", "All")
	elseif Mode == "Murderous" then Chat "I can taste the blood..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Murderous] I can taste the blood...  ", "All")
	elseif Mode == "STRESSED" then Chat "..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [STRESSED] I'm really done with you.. ", "All")
					wait (2)
					game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [STRESSED] YOU'RE EXISTING IS WASTED!! ", "All")
	elseif Mode == "GALACTIA" then Chat "You think your alone..."
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [G a l a c t i a] You think your alone...  ", "All")
	elseif Mode == "Overclocked" then Chat "!"
	elseif Mode == "Volcanic" then Chat "RAINING FIRE"
	                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Volcanic] RAINING FIRE ", "All")
	elseif Mode == "HELLSCAPE" then Chat "..."
	elseif Mode == "sus" then Chat "When the imposter is sus:"
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [sus] When the imposter is sus  ", "All")	
	elseif Mode == "S P A C E T I M E" then Chat "."	    
	    
	    
	    
	    
	    
	    
	    
end
end



UIS.InputBegan:connect(function(io,gpe)
	if(gpe or Attack or data.User~=data.Local)then return end
	--MODES
	if(io.KeyCode == Enum.KeyCode.One and Mode~='Neutral')then 
		changeMode'Neutral'  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.Two and Mode~='Love')then 
		changeMode'Love'  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.Three and Mode~='CRUELTY')then
		changeMode'CRUELTY' VaporTaunt()  game.lighting.ClockTime = 4 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.N and Mode~='CHAOS')then 
		changeMode'CHAOS'  game.lighting.ClockTime = 0 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.X and Mode~=(e2.Text))then 
		changeMode(e2.Text)  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.B and Mode~='DICK')then
		changeMode'DICK'  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.P and Mode~='Interstellar')then 
		changeMode'Interstellar'  game.lighting.ClockTime = 0 game.Lighting.FogEnd = 9999
		           game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Interstellar] The universe is at my hands! ", "All")
	elseif(io.KeyCode == Enum.KeyCode.G and Mode~='Overclocked')then
		changeMode'Overclocked' game.lighting.ClockTime = 10 game.Lighting.FogEnd = 999999999999
		Attack = true
	NeutralAnims = false
	local orig=WalkSpeed
	WalkSpeed=0
	legAnims=false
	Chat"Hyper power!"

	
	for i = 0, 5, 0.1 do
		swait(1)
		local Alpha = .1
				RJ.C0 = RJ.C0:lerp(CF.N(0,2.4+.2*M.C(Sine/24),0)*CF.A(M.R(60+5*M.S(Sine/24)),M.R(0),M.R(0)),Alpha)
				LS.C0 = LS.C0:lerp(LSC0*CF.N(-0.,-0+.05*M.S(Sine/32),0)*CF.A(M.R(-60),M.R(5+0*M.C(Sine/32)),M.R(0-1*M.C(Sine/32))),Alpha)
				RS.C0 = RS.C0:lerp(RSC0*CF.N(0.,-0+.05*M.S(Sine/32),0)*CF.A(M.R(-60),M.R(5-0*M.C(Sine/32)),M.R(-0+1*M.C(Sine/32))),Alpha)
				NK.C0 = NK.C0:lerp(CF.N(0,1.4,-0.5)*CF.A(M.R(-40-5*M.S(Sine/24)),M.R(-18.7),M.R(-3.7)),Alpha)

	end
	coroutine.wrap(function()
		for i = 0, 5 do
			Camshake({
			    Duration=.2;
				FadeOut=.2;
			    Intensity=1.5;
			    Position=Vector3.new(.5,.5,.5);
			    Rotation=Vector3.new(.5,.5,5);
			    DropDist=1;
			    IneffectiveDist=40;
			    Origin=Root.CFrame*CF.N(0,0,-4-i*4);
			})
			SoundPart(551132916,.8,4,false,true,true,Root.CFrame*CF.N(0,0,-4-i*4))
			AOEDamage(Root.CFrame*CF.N(0,0,-4-i*4).p,5,{
				DamageColor=PrimaryColor;
				MinimumDamage=25;
				MaximumDamage=45;
			})
			Effect{
				Lifetime=.2;
					Mesh={Type=Enum.MeshType.Sphere};
					Color=PrimaryColor;
					Transparency={0,1};
					Material=Enum.Material.Neon;
					Size=Vector3.new(1,1,1);
					EndSize=Vector3.new(200,200,200);
			}
			Effect{
				Lifetime=.2;
					Mesh={Type=Enum.MeshType.Sphere};
					Color=PrimaryColor;
					Transparency={0,1};
					Material=Enum.Material.Neon;
					Size=Vector3.new(50,50,50);
					EndSize=Vector3.new(200,200,200);
			}
			Effect{
				Lifetime=.2;
					Mesh={Type=Enum.MeshType.Sphere};
					Color=PrimaryColor;
					Transparency={0,1};
					Material=Enum.Material.Neon;
					Size=Vector3.new(100,100,100);
					EndSize=Vector3.new(200,200,200);
			}
			swait(4)
		end
		
	end)()
		for i = 0, 5, 0.1 do
		swait(3)
		local Alpha = .1
		RJ.C0 = RJ.C0:lerp(CF.N(0,0+30,0.7)*CF.A(M.R(18.2),M.R(350+360),M.R(0)),Alpha)
		NK.C0 = NK.C0:lerp(NKC0,Alpha)
		LS.C0 = LS.C0:lerp(LSC0*CF.N(0,0+.2*M.S(Sine/52),0)*CF.A(0,M.R(5+5*M.C(Sine/52)),M.R(-135-15*M.C(Sine/62))),Alpha)
		RS.C0 = RS.C0:lerp(RSC0*CF.N(-0,0+.2*M.S(Sine/52),-0)*CF.A(0,M.R(5-5*M.C(Sine/52)),M.R(135+15*M.C(Sine/62))),Alpha)
		RS.C0 = RS.C0:lerp(CF.N(1.4,0.5,0.2)*CF.A(M.R(89.5),M.R(2.6),M.R(50)),Alpha)

	end

	WalkSpeed=orig
	legAnims=true
	Attack=false
	NeutralAnims=true
	
