end

WingAnims.FUTUREWings=function()
	LWP1W.C0 = LWP1W.C0:lerp(CF.N(1,1.5,-3)*CF.A(M.R(15+0*M.C(WingSine/32)),0,M.R(-30-8000*M.C(WingSine/232))),.2)
