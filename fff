local MPASword = {}
for _,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
	if v:IsA("Accessory") and v.Name:find("MeshPartAccessory") then
		table.insert(MPASword,v)
	end
end



local LWP1 = WingPiece:Clone();
