	elseif(io.KeyCode == Enum.KeyCode.F and Mode=='I.n.S.a.N.i.T.y')then 
		changeMode'...'  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.T and Mode=='Galaxy')then 
		changeMode'Galactic' VaporTaunt()  game.lighting.ClockTime = 0 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.F and Mode=='GALACTIA')then 
		superpowerup()	changeMode'SPACETIME' VaporTaunt()   game.lighting.ClockTime = 0 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.R and Mode=='GALACTIA')then 
		changeMode'GALVANIC' VaporTaunt()   game.lighting.ClockTime = 0 game.Lighting.FogEnd = 999999999999
		           game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [GALVANIC] I Won't Leave This To Rest ", "All")
	elseif(io.KeyCode == Enum.KeyCode.F and Mode=='...')then 
		changeMode'I.n.S.a.N.i.T.y'  game.lighting.ClockTime = 4 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.T and Mode=='Volcanic')then 
		changeMode'Inferno'  game.lighting.ClockTime = 0 game.Lighting.FogEnd = 999999999999
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Inferno] BURN, BURN! ", "All")
	elseif(io.KeyCode == Enum.KeyCode.F and Mode=='Radioactivity')then
		changeMode'ULTRAVIOLET' VaporTaunt()  game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.M and Mode=='MAYHEM')then
		changeMode'Murderous' VaporTaunt()   game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
	elseif(io.KeyCode == Enum.KeyCode.H and Mode=='O B L I V I O N')then 
		changeMode'???'  VaporTaunt()   game.lighting.ClockTime = 1 game.Lighting.FogEnd = 999999999999
		
			elseif(io.KeyCode == Enum.KeyCode.F and Mode=='CHAOS')then 
		changeMode'HOLY'  VaporTaunt()   game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		           game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("                                                                                                                                            [Holy] How the tables turn. ", "All")

	elseif(io.KeyCode == Enum.KeyCode.T and Mode=='BEYOND')then
		changeMode'The Doctor' VaporTaunt()
	elseif(io.KeyCode == Enum.KeyCode.T and Mode=='The Doctor')then
		changeMode'ILLUMINATION' VaporTaunt()
	--TOGGLE MUSIC
	elseif(io.KeyCode == Enum.KeyCode.M and getMode(Mode))then 
		MusicMode=MusicMode+1
		if(MusicMode>3)then MusicMode=1 end
		if(MusicMode==1)then
			music:Pause()
			music.Volume=5
			music.Parent=Torso
			music:Resume()
		elseif(MusicMode==2)then
			music:Pause()
			music.Volume=5
			music.Parent=Char
			music:Resume()
		elseif(MusicMode==3)then
			music.Volume = 0
		end
	elseif(io.KeyCode==Enum.KeyCode.R)then
		--TAUNTS
		if(vaporwaveMode and Mode=='Troubadour')then
			VaporTaunt()
		end
	end
	if(vaporwaveMode)then return end
	--ATTACKS
	if(io.UserInputType==Enum.UserInputType.MouseButton1)then
		ClickCombo()
	end
end)

WingAnims.StarG=function()
 LWP1W.C0 = LWP1W.C0:lerp(CF.N(4*M.C(WingSine/32),1.5+.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(0+45*M.C(WingSine/12))),.2)
    LWP2W.C0 = LWP2W.C0:lerp(CF.N(6*M.C(WingSine/32),1+.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(90+45*M.C(WingSine/12))),.2)
    LWP3W.C0 = LWP3W.C0:lerp(CF.N(8*M.C(WingSine/32),.5+.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(180+45*M.C(WingSine/12))),.2)

    RWP1W.C0 = RWP1W.C0:lerp(CF.N(-4*M.C(WingSine/32),1.5-.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(-0-45*M.C(WingSine/12))),.2)
    RWP2W.C0 = RWP2W.C0:lerp(CF.N(-6*M.C(WingSine/32),1+.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(-90-45*M.C(WingSine/12))),.2)
    RWP3W.C0 = RWP3W.C0:lerp(CF.N(-8*M.C(WingSine/32),.5+.1*M.S(WingSine/32),-1)*CF.A(0.2,0,M.R(-180-45*M.C(WingSine/12))),.2)  
    RWP4W.C0 = RWP4W.C0:lerp(CF.N(2*M.C(WingSine/32),10+1*M.C(WingSine/32),-1)*CF.A(0,0,M.R(90-300*M.C(WingSine/50))),.2)
    RWP5W.C0 = RWP5W.C0:lerp(CF.N(2*M.C(WingSine/32),10+1*M.C(WingSine/32),-1)*CF.A(0,0,M.R(180-300*M.C(WingSine/50))),.2)
    RWP6W.C0 = RWP6W.C0:lerp(CF.N(2*M.C(WingSine/32),10+1*M.C(WingSine/32),-1)*CF.A(0,0,M.R(270-300*M.C(WingSine/50))),.2)
